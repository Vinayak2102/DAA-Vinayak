\\using jump search to find different numbers in different arrays
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <math.h>

int jump(int arr[], int size, int key)
{
  int i=0;
  int s=sqrt(size);
  while(arr[s]<=key && s<size)
  {
    i=s;
    s+=sqrt(size);
    if(s>size-1)
    {
      s=size;
    }
  }
  for(i=i;i<s;i++)
  {
    if(arr[i]==key)
    {
      return i+1;
    }
  }
  return -1;
}

int main()
{
  int n, j, i, size, x=0, key;
  int arr[20];
  int ans[10];
  scanf("%d", &n);
  for(i=0;i<n;i++)
  {
    scanf("%d", &size);
    for(j=0;j<size;j++)
    {
      scanf("%d", &arr[j]);
    }
    scanf("%d", &key);
    ans[x]=jump(arr, size, key);
    x++;
  }
  for(i=0;i<x;i++)
  {
    if(ans[i]==-1)
    {
      printf("Not found\n");
    }
    else
    {
      printf("Found at %d\n", ans[i]);
    }
  }
  return 0;
}

